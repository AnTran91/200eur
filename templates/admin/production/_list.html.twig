
<!-- search form -->
{% trans_default_domain 'admin' %}
<!-- DATA TABLE -->
<div class="card">
  <div class="table-responsive">
    <table class="table card-table table-vcenter text-nowrap">
      <thead>
        <tr>
          <th>{{ pagination_sortable(productions, 'admin.production.country'|trans, 'production.country') }}</th>
          <th></th>
        </tr>
        <tr class="spacer"></tr>
      </thead>
      <tbody>
        {% for production in productions %}
          <tr>
          <td>{{ production.country }}</td>
            <td>
              <div class="table-data-feature">

                {% include 'admin/production/_delete_form.html.twig' %}

                <a href="{{ path('admin_production_show', {'id': production.id}) }}" class="item" data-toggle="tooltip" data-placement="top" title="{{ 'admin.common.action.show'|trans }}" data-original-title="More">
                  <i class="zmdi zmdi-eye"></i>
                </a>
                <a href="{{ path('admin_production_edit', {'id': production.id}) }}" class="item" data-toggle="tooltip" data-placement="top" title="{{ 'admin.common.action.edit'|trans }}" data-original-title="More">
                  <i class="fe fe-edit mr-1"></i>
                </a>
              </div>
            </td>
          </tr>
        {% else %}
          <tr class="odd">
            <td valign="top" colspan="7" class="text-center">{{ 'admin.paginator.empty_table'|trans|raw }}</td>
          </tr>
        {% endfor %}
      </tbody>
    </table>
  </div>
  <div class="card-footer p-0">
    <div class="d-flex align-items-center p-2">
      <div class="mr-auto p-2">
        {{ pagination_info(productions) }}
      </div>
      <div class="p-2">

        {{ knp_pagination_render(productions) }}
      </div>
    </div>
  </div>
</div>
