<form method="post" action="{{ path('network_delete', {'id': network.id}) }}">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ network.id) }}">
    <button class="btn delete-btn  js-confirm-alert"><i class="zmdi zmdi-delete"></i></button>
</form>
