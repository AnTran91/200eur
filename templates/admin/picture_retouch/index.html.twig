{% extends 'admin/base.html.twig' %}

{% block title %}PictureDetails index{% endblock %}

{% block body %}
    <h1>PictureDetails index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Price</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for picture_retouch in picture_retouches %}
            <tr>
                <td>{{ picture_retouch.id }}</td>
                <td>{{ picture_retouch.price }}</td>
                <td>
                    <a href="{{ path('admin_picture_retouch_show', {'id': picture_retouch.id}) }}">show</a>
                    <a href="{{ path('admin_picture_retouch_edit', {'id': picture_retouch.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('admin_picture_retouch_new') }}">Create new</a>
{% endblock %}
